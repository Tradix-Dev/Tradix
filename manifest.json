{
  "manifest_version": 3,
  "name": "Tradix",
  "version": "1.0.0",
  "description": "Automated Solana copy trading.",
  "permissions": [
    "storage"
  ],

  "action": {
    "default_popup": "popup.html",
    "default_title": "Tradix",
    "default_icon": {
      "16": "icons/icon16.png",
      "32": "icons/icon32.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  },
  "background": {
    "service_worker": "background.js"
  },

  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}
